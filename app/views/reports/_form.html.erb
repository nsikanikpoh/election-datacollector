       <%= form.text_field :pdp_votes, type:"number", class:"form-control", required:"required" %>
    
  </div>
          </div>

       <div class="col-md-6">
   <div class="form-group">
  <%= form.label :prp_votes, "PRP Votes" %>
       <%= form.text_field :prp_votes, type:"number", class:"form-control", required:"required" %>
    
  </div>
          </div>


             <div class="col-md-6">
   <div class="form-group">
  <%= form.label :ypp_votes, "YPP Votes" %>
       <%= form.text_field :ypp_votes, type:"number", class:"form-control", required:"required" %>
    
  </div>
          </div>


   <div class="col-md-6">
   <div class="form-group">
  <%= form.label :total_votes, "Total Votes for all Parties" %>
       <%= form.text_field :total_votes, type:"number", class:"form-control", required:"required" %>
    
  </div>
          </div>



         <div class="col-md-6">
   <div class="form-group">
  
       <%= form.label :result_time, "Time Result Announced" %>
  <%= form.time_field :result_time,
      value: "%H:%M",
      min: 'hh:mm:ss',
      max: 'hh:mm:ss', class:"form-control"
  %>
    
  </div>
          </div>


 <div class="col-md-6">
   <div class="form-group">
  
       <%= form.label :pdp_agent, "PDP Agent" %>
<%= form.text_field :pdp_agent,  class:"form-control has-feedback-left", placeholder:"Name", type:"text", required:"required" %>
    
  </div>
          </div>


           <div class="col-md-6">
   <div class="form-group">
  <%= form.label :agent_phone, "Agent Phone" %>
       <%= form.text_field :agent_phone, type:"number", class:"form-control", required:"required" %>
  </div></div>



           <div class="col-md-6">
   <div class="form-group">
  
       <%= form.label :officer_name, "Name of Presiding Officer" %>
<%= form.text_field :officer_name,  class:"form-control", placeholder:"Name", type:"text", required:"required" %>
    
  </div>
          </div>


                     <div class="col-md-6">
   <div class="form-group">
  
       <%= form.label :officer_gender, "Gender of Presiding Officer" %>
     <%= form.select :officer_gender, ["Male","Female"], placeholder:"Title", required:"required"%>
    
  </div>
          </div>







          <div class="form-group">
            <div class="col-md-9 col-sm-9 col-xs-12 col-md-offset-3">
               <%= form.submit "Submit Report", class:"btn btn-info"%>  

</div>
</div>
</div>
</form>
</div>
</div>


<% end %>
